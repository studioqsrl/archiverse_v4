# Public configuration
NEXT_PUBLIC_AUTH0_DOMAIN=your-auth0-domain.auth0.com
NEXT_PUBLIC_CUSTOM_CLAIMS_NAMESPACE=https://archiverse.io

# Development-only secrets (do not use in production)
# Production uses K8s secrets synced from Azure KeyVault
AUTH0_CLIENT_ID=
AUTH0_MANAGEMENT_CLIENT_ID=
AUTH0_ADMIN_ROLE_ID=
AUTH0_MEMBER_ROLE_ID=
DEFAULT_CONNECTION_ID=

# Database configuration (development only)
POSTGRES_HOST=localhost
POSTGRES_DB=archiverse
POSTGRES_USER=postgres
POSTGRES_PASSWORD=development-password-only

# Production secrets (managed by K8s + Azure KeyVault)
# AUTH0_CLIENT_SECRET=
# AUTH0_MANAGEMENT_CLIENT_SECRET=
# SESSION_ENCRYPTION_SECRET=
